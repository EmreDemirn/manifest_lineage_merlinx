<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <!-- remote list -->
    <remote
        name="sunfish"
        fetch="https://github.com/team-sunfish"
        revision="lineage-21"
    />
    <remote name="lineage"
        fetch="https://github.com/LineageOS"
        revision="lineage-21"
    />

    <!-- merlinx -->
    <project path="device/xiaomi/merlinx" name="device_xiaomi_merlinx" remote="sunfish"
        revision="lineage-21" 
    />
    <project path="vendor/xiaomi/merlinx" name="vendor_xiaomi_merlinx" remote="sunfish"
        revision="lineage-21" 
    />

    <!-- device -->
    <project path="device/mediatek/sepolicy_vndr" name="android_device_mediatek_sepolicy_vndr" remote="lineage"
        revision="lineage-21"
    />

    <!-- commons -->
    <project path="device/xiaomi/mt6768-common" name="device_xiaomi_mt6768-common" remote="sunfish"
        revision="lineage-21" 
    />
    <project path="vendor/xiaomi/mt6768-common" name="vendor_xiaomi_mt6768-common" remote="sunfish"
        revision="lineage-21" 
    />

    <!-- kernel source -->
    <project path="kernel/xiaomi/mt6768" name="kernel_xiaomi_mt6768" remote="sunfish"
        revision="lineage-21-fixes-openela-bpf" 
    />

    <!-- hardware -->
    <project path="hardware/mediatek" name="android_hardware_mediatek" remote="lineage"
        revision="lineage-21" 
    />
    <project path="hardware/xiaomi" name="android_hardware_xiaomi" remote="lineage"
        revision="lineage-21" 
    />
</manifest>
